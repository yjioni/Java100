public class Java100_operator_LogicalOperators{
	public static void main(String[] args){
		
		// &&	and
		// ||	or
		// !	not
		
		int a=10, b=20, c=40;
		
		boolean rst1, rst2, rst3;
		rst1 = a>=10 && b<=c;		
		System.out.println(rst1);
		
		rst2 = a==c || b<30;
		System.out.println(rst2);
		
		rst3 = !rst2;
	}
}